package com.hdi.animation.easing{		////////////////////////////////////////////////////////////////////////////////	//	Reverse 1.0 1/30/11	//  Copyright © Todd Williams	//	All rights reserved.	//		////////////////////////////////////////////////////////////////////////////////	public class Arch{				public function Arch(){		}				//Arch		public static function easeOut(t:Number, b:Number, c:Number, d:Number):Number {			var e:Number = (Math.sin(t/d * Math.PI/2) - 1)*2.25;			var rad:Number = t/d*180;			return c * Math.sin(rad*Math.PI/180) * -e  + b;					}		public static function easeIn(t:Number, b:Number, c:Number, d:Number):Number {			var e:Number = (Math.cos(Math.PI*t/d) - 1)*0.765;			var rad:Number = t/d*180;			return c * Math.sin(rad*Math.PI/180) * -e + b;					}		public static function easeInOut(t:Number, b:Number, c:Number, d:Number):Number {			var e:Number = Math.abs((Math.cos(Math.PI*t/d) - 1));			var rad:Number = t/d*180;			if(rad <= 90){				return c * Math.sin(rad*Math.PI/180) * e + b;			}			else{				return c * Math.sin(rad*Math.PI/180) * -e + b;			} 		}				public static function easeOutIn(t:Number, b:Number, c:Number, d:Number):Number {			var e:Number = Math.abs((Math.sin(Math.PI*t/d) - 1));			var rad:Number = t/d*180;			if(rad <= 90){				return c * Math.cos(rad*Math.PI/180) * -e + c + b;			}			else{				return -c * Math.cos(rad*Math.PI/180) * -e + c + b;			} 		}				public static function linear(t:Number, b:Number, c:Number, d:Number):Number{						var rad:Number = t/d*180;			return c *  Math.sin(rad*Math.PI/180) + b;		}	}}